{
    "version": 2,
    "builds": [
        {
            "src": "api/index.mjs",
            "use": "@vercel/node"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.mjs"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}